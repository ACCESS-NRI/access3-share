spack:
  specs:
  - access3 @git.{{ ref }} configurations={{ all_configurations }}
  packages:
    access3-share:
      require:
        '@git.{{ ref }}=stable'
    gcc-runtime:
      require:
        '%gcc'
    all:
      require:
      - '%{{ oneapi_compiler }} target={{ target }}'
  concretizer:
    unify: false
  view: false
